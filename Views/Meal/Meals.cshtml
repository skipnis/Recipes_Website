@model List<Meal>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<html>
<head>
    <title>title</title>
</head>
<body>
<div class="container mt-4">
    <h1 class="mb-4r text-center">Meals</h1>
    <ul class="list-group">
        @foreach (var meal in Model)
        {
        <a asp-controller="Meal" asp-action="Details" asp-route-categoryId="@meal.Id" class="list-group-item list-group-item-action">
            @meal.Name
        </a>
        }
    </ul>
</div>
</body>
</html>
